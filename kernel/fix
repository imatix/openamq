for FILE in $*; do
    if test -f $FILE.icl; then
        for a in $FILE*.icl; do
            gsl -q $a;
        done
    fi
    if test -f $FILE.smt; then
        for a in $FILE*.smt; do
            gsl -q $a
        done
    fi
    if test -f $FILE.xml; then
        for a in $FILE*.xml; do
            gsl -q $a
        done
    fi
    if test -f $FILE.c; then
        c $FILE.c
        test $? -ne 0 && exit
        c -r libamq $FILE.o
        test -f $FILE_test.c && c -l $FILE_test
    fi
    if test -f $FILE; then
        c -L $FILE
    fi
done
