    c $MY_COPTS -r libgsl ggdiag || croak "\"c $MY_COPTS -r libgsl ggdiag\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -r libgsl ggenvt"
    c $MY_COPTS -r libgsl ggenvt || croak "\"c $MY_COPTS -r libgsl ggenvt\" failed"
