        FREE_SLOTS=`expr $FREE_SLOTS - 1`
    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS ggscop"
    if [ -z "$BOOM_PIPELINE" ]; then
        c $MY_COPTS ggscop || croak "\"c $MY_COPTS ggscop\" failed"
    else
        if [ "$FREE_SLOTS" = "0" ]; then
