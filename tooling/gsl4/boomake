        fi
        c $MY_COPTS ggdiag || croak "\"c $MY_COPTS ggdiag\" failed" &
        FREE_SLOTS=`expr $FREE_SLOTS - 1`
    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS ggenvt"
