        FREE_SLOTS=`expr $FREE_SLOTS - 1`
    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS ggproc"
    if [ -z "$BOOM_PIPELINE" ]; then
        c $MY_COPTS ggproc || croak "\"c $MY_COPTS ggproc\" failed"
    else
        if [ "$FREE_SLOTS" = "0" ]; then
            wait
            FREE_SLOTS=$BOOM_PIPELINE
        fi
        c $MY_COPTS ggproc || croak "\"c $MY_COPTS ggproc\" failed" &
        FREE_SLOTS=`expr $FREE_SLOTS - 1`
