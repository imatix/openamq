            FREE_SLOTS=$BOOM_PIPELINE
        fi
        c $MY_COPTS ggconv || croak "\"c $MY_COPTS ggconv\" failed" &
        FREE_SLOTS=`expr $FREE_SLOTS - 1`
    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS ggdiag"
    if [ -z "$BOOM_PIPELINE" ]; then
