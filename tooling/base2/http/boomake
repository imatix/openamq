    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L http_driver_null_test"
    if [ -z "$BOOM_PIPELINE" ]; then
        c $MY_COPTS -L http_driver_null_test || croak "\"c $MY_COPTS -L http_driver_null_test\" failed"
    else
