    fi
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L http_driver_cgi_test"
    if [ -z "$BOOM_PIPELINE" ]; then
        c $MY_COPTS -L http_driver_cgi_test || croak "\"c $MY_COPTS -L http_driver_cgi_test\" failed"
    else
        if [ "$FREE_SLOTS" = "0" ]; then
            wait
            FREE_SLOTS=$BOOM_PIPELINE
        fi
        c $MY_COPTS -L http_driver_cgi_test || croak "\"c $MY_COPTS -L http_driver_cgi_test\" failed" &
