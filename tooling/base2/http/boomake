    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 http_response.icl"
    gsl -q -quiet:1 http_response.icl || croak "\"gsl -q -quiet:1 http_response.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 http_driver_module.icl"
