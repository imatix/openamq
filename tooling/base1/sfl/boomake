    cat sfltree.h>>sfl.h || croak "\"cat sfltree.h>>sfl.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cat sfltron.h>>sfl.h"
    cat sfltron.h>>sfl.h || croak "\"cat sfltron.h>>sfl.h\" failed"
